{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "packageRules": [
    {
      "packageNames": [
        "johnpbloch/wordpress"
      ],
      "labels": [
        "wordpress",
        "wordpress-core"
      ]
    },
    {
      "packageNames": [
        "wp-cli/wp-cli"
      ],
      "labels": [
        "wordpress",
        "wp-cli",
        "tools"
      ]
    },
    {
      "matchPackagePrefixes": [
        "wpackagist-plugin/"
      ],
      "groupName": "WordPress Plugins",
      "addLabels": [
        "wordpress",
        "wordpress-plugin"
      ]
    }
  ]
}
